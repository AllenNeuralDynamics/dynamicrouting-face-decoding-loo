{
	"version": 1,
	"base_image": "codeocean/mambaforge3:23.1.0-4-python3.10.12-ubuntu22.04",
	"args": [
		"API_KEY",
		"API_SECRET",
		"AWS_ACCESS_KEY_ID",
		"AWS_DEFAULT_REGION",
		"AWS_SECRET_ACCESS_KEY"
	],
	"options": {
		"registry_host_arg": true,
		"git_ask_pass": true
	},
	"installers": {
		"apt": {
			"packages": [
				{
					"name": "build-essential",
					"version": "12.9ubuntu3"
				}
			],
			"install_software_properties_common": true
		},
		"pip3": {
			"packages": [
				{
					"name": "aind-data-schema",
					"version": "1.1.1"
				},
				{
					"name": "aind-data-schema-models",
					"version": "0.5.7"
				},
				{
					"name": "code-ocean-aux-tools",
					"version": "1.1.3"
				},
				{
					"name": "git+https://github.com/AllenInstitute/dynamic_routing_analysis#egg=dynamic-routing-analysis",
					"version": "ad88479ed99947a17967266f32e4a56843ab9c3e"
				},
				{
					"name": "hdmf",
					"version": "3.14.6"
				},
				{
					"name": "numba",
					"version": "0.60.0"
				},
				{
					"name": "pandas[all]",
					"version": "2.2.3"
				},
				{
					"name": "polars[plot,deltalake]",
					"version": "1.26.0"
				},
				{
					"name": "pydantic",
					"version": "2.7.4"
				},
				{
					"name": "pynwb",
					"version": "2.8.2"
				},
				{
					"name": "s3fs",
					"version": "2024.10.0"
				},
				{
					"name": "scikit-learn",
					"version": "1.5.2"
				},
				{
					"name": "universal-pathlib",
					"version": "0.1.4"
				},
				{
					"name": "zarr",
					"version": "2.17.2"
				}
			],
			"options": {},
			"pre_install_options": {}
		}
	}
}